// prisma/schema.prisma

generator client {
  provider = "prisma-client-js"
  output   = "../node_modules/.prisma/client"

}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Usuarios {
  id       Int    @id @default(autoincrement())
  nome     String
  email    String @unique
  senha    String
  foto     String?
}